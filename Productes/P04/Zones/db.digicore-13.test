;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.digicore-13.test. usuari.digicore-13.test. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns1.digicore-13.test.

; registres
ns1		IN	A	192.168.4.13
server		IN	A	192.168.4.138
dbserver	IN	A	192.168.4.113
data		IN	CNAME	dbserver
